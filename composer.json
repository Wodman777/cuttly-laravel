{
    "name": "toneflix-code/cuttly-laravel",
    "description": "A Laravel API wrapper for Cutt.ly API.",
    "version": "1.0.0",
    "keywords": [
        "toneflix-code",
        "cuttly-laravel"
    ],
    "homepage": "https://github.com/toneflix-code/cuttly-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Toneflix Code",
            "email": "code@toneflix.com.ng",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ToneflixCode\\Cuttly\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ToneflixCode\\Cuttly\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ToneflixCode\\Cuttly\\CuttlyServiceProvider"
            ],
            "aliases": {
                "Cuttly": "ToneflixCode\\Cuttly\\CuttlyFacade"
            }
        }
    }
}
